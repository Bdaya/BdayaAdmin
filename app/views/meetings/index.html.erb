<h1>Meetings</h1>
<p id="notice"><%= notice %></p>
<table>
  <tr>
    <th>Time</th>
    <th>Location</th>
    <th>Meeting minuts</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @meeting.each do |meeting| %>
  <tr>
    <td><%= meeting.time %></td>
    <td><%= meeting.location %></td>
    <td><%= meeting.meeting_minuts %></td>
    <td><%= link_to 'Show Meeting',meeting %></td>
    <td><%= link_to 'Cancel Meeting', meeting ,confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Create a Meeting', new_meeting_path %>